<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>highway-service</display-name>
    <servlet>
  	<description>JAX-WS endpoint - MyUserService</description>
  	<display-name>MyUserService</display-name>
  	<servlet-name>MyUserService</servlet-name>
  	<servlet-class>
  		com.sun.xml.ws.transport.http.servlet.WSServlet
  	</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
  	<description>JAX-WS endpoint - AreaService</description>
  	<display-name>AreaService</display-name>
  	<servlet-name>AreaService</servlet-name>
  	<servlet-class>
  		com.sun.xml.ws.transport.http.servlet.WSServlet
  	</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
  	<description>JAX-WS endpoint - EquipmentTableService</description>
  	<display-name>EquipmentTableService</display-name>
  	<servlet-name>EquipmentTableService</servlet-name>
  	<servlet-class>
  		com.sun.xml.ws.transport.http.servlet.WSServlet
  	</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
  	<description>JAX-WS endpoint - NodeLogService</description>
  	<display-name>NodeLogService</display-name>
  	<servlet-name>NodeLogService</servlet-name>
  	<servlet-class>
  		com.sun.xml.ws.transport.http.servlet.WSServlet
  	</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
  	<servlet-name>NodeLogService</servlet-name>
  	<url-pattern>/NodeLogService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EquipmentTableService</servlet-name>
  	<url-pattern>/EquipmentTablePort</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
  	<servlet-name>MyUserService</servlet-name>
  	<url-pattern>/MyUserPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AreaService</servlet-name>
  	<url-pattern>/AreaPort</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>

  <listener>
  	<listener-class>
  		com.sun.xml.ws.transport.http.servlet.WSServletContextListener
  	</listener-class>
  </listener>
  <listener>
  	<listener-class>
  		com.sun.xml.ws.transport.http.servlet.WSServletContextListener
  	</listener-class>
  </listener>
  <listener>
  	<listener-class>
  		com.sun.xml.ws.transport.http.servlet.WSServletContextListener
  	</listener-class>
  </listener>
  <listener>
  	<listener-class>
